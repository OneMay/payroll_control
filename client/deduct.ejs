<%include  header.ejs %>
    <div class=" admin-content">
        <div class="daohang">
        </div>
        <div class="admin-biaogelist">

            <div class="listbiaoti am-cf">
                <ul class="am-icon-flag on"> 栏目名称</ul>

                <dl class="am-icon-home" style="float: right;"> 当前位置： 首页 > <a href="#">工资扣除</a></dl>
                <dl>
                    <button type="button" class="am-btn am-btn-danger am-round am-btn-xs am-icon-plus"> 工资扣除</button>
                </dl>
            </div>

            <div class="fbneirong">
                <form class="am-form">
                    <div class="am-form-group am-cf">
                        <div class="zuo">员工编号 </div>
                        <div class="you">
                            <select name="sex" id="employeeNo" class="am-input-sm deduction">
                             </select>
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">扣除工资时长</div>
                        <div class="you">
                            <input type="text" class="am-input-sm deduction" id="deductDate" placeholder="扣除工资时长">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">扣除工资金额</div>
                        <div class="you">
                            <input type="text" class="am-input-sm deduction" id="deductAmount" placeholder="扣除工资金额">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">扣除类型编号</div>
                        <div class="you">
                            <select name="deductTypeNo" id="deductTypeNo" class="am-input-sm deduction">
                            
                            </select>
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="you" style="margin-left: 11%;">
                            <span id="deductAdd" class="am-btn am-btn-success am-radius">添加</span>
                            <span class="am-form-help" id="Message" style="display:inline-block;"></span>
                        </div>
                    </div>
                    <div class="deductType">
                        <hr />
                        <p class="Message"></p>
                    </div>
                </form>
            </div>
            <%include  footer.ejs %>