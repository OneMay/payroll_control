<%include  header.ejs %>
    <div class=" admin-content">
        <div class="daohang">
        </div>
        <div class="admin-biaogelist">

            <div class="listbiaoti am-cf">
                <ul class="am-icon-flag on"> 栏目名称</ul>

                <dl class="am-icon-home" style="float: right;"> 当前位置： 首页 > <a href="#">奖金发放</a></dl>
                <dl>
                    <button type="button" class="am-btn am-btn-danger am-round am-btn-xs am-icon-plus"> 奖金发放</button>
                </dl>
            </div>

            <div class="fbneirong">
                <form class="am-form">
                    <div class="am-form-group am-cf">
                        <div class="zuo">员工编号 </div>
                        <div class="you">
                            <select name="sex" id="employeeNo" class="am-input-sm bonus">
                             </select>
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">奖金发放日期</div>
                        <div class="you">
                            <input type="date" class="am-input-sm bonus" id="bonusDate">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">奖金金额</div>
                        <div class="you">
                            <input type="text" class="am-input-sm bonus" id="bonusAmount" placeholder="奖金金额">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">奖金类型编号</div>
                        <div class="you">
                            <select name="bonusTypeNo" id="bonusTypeNo" class="am-input-sm bonus">
                            
                            </select>
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="you" style="margin-left: 11%;">
                            <span id="bonusAdd" class="am-btn am-btn-success am-radius">添加</span>
                            <span class="am-form-help" id="Message" style="display:inline-block;"></span>
                        </div>
                    </div>
                    <div class="bonusSend">
                        <hr />
                        <p class="Message"></p>
                    </div>
                </form>
            </div>
