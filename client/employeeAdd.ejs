<%include  header.ejs %>
    <div class=" admin-content">
        <div class="daohang">
        </div>
        <div class="admin-biaogelist">

            <div class="listbiaoti am-cf">
                <ul class="am-icon-flag on"> 栏目名称</ul>

                <dl class="am-icon-home" style="float: right;"> 当前位置： 首页 > <a href="#">信息添加</a></dl>
                <dl>
                    <button type="button" class="am-btn am-btn-danger am-round am-btn-xs am-icon-plus"> 信息添加</button>
                </dl>
            </div>

            <div class="fbneirong">
                <form class="am-form">
                    <div class="am-form-group am-cf">
                        <div class="zuo">职位</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="title" placeholder="职位">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">姓氏</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="fistName" placeholder="姓氏">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">中间名</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="middleName" placeholder="中间名">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">最后名</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="lastName" placeholder="最后名">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">地址</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="address" placeholder="地址">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">公司电话分机号</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="workTelExt" placeholder="公司电话分机号">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">家用电话号码</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="homTelNo" placeholder="家用电话号码">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">Email地址</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="empEmailAdress" placeholder="Email地址">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">社保号码</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="socialSecurutyNumber" placeholder="社保号码">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">出生日期</div>
                        <div class="you">
                            <input type="date" class="am-input-sm employeeList" id="DOB" placeholder="出生日期">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">部门</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="position" placeholder="部门">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">性别</div>
                        <div class="you">
                            <select name="sex" id="sex" class="am-input-sm employeeList">
                                <option value="男">男</option>
                                <option value="女">女</option>
                            </select>
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">工资</div>
                        <div class="you">
                            <input type="text" class="am-input-sm employeeList" id="salary" placeholder="工资">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="zuo">开始时间</div>
                        <div class="you">
                            <input type="date" class="am-input-sm employeeList" id="dateStarted" placeholder="开始时间">
                        </div>
                    </div>
                    <div class="am-form-group am-cf">
                        <div class="you" style="margin-left: 11%;">
                            <span id="employeeAdd" class="am-btn am-btn-success am-radius">发布</span>
                            <span class="am-form-help" id="Message" style="display:inline-block;"></span>
                        </div>
                    </div>
                </form>
            </div>
            <%include  footer.ejs %>